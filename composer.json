{
    "description": "Integrate MXGraph / Draw.io into Neos as Node Type for creating interactive diagrams and flowcharts",
    "type": "neos-plugin",
    "name": "sandstorm/mxgraph",
    "require": {
        "typo3/neos": "<=2.3.9999"
    },
    "autoload": {
        "psr-4": {
            "Sandstorm\\MxGraph\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Sandstorm.MxGraph"
        },
        "applied-flow-migrations": [
            "TYPO3.FLOW3-201201261636",
            "TYPO3.Fluid-201205031303",
            "TYPO3.FLOW3-201205292145"
        ]
    }
}